{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "plugin:react/jsx-runtime",
        "standard-with-typescript"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module",
        "dot-notation": "warn",
        "project": [
            "tsconfig.json"
        ]
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "quotes": [
            2,
            "single",
            {
                "avoidEscape": true
            }
        ]
    }
}